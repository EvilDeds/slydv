{
  "name": "slydv",
  "version": "1.0.0",
  "description": "Slides for Developers",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "first-deploy": "webpack -p && git add -f public/bundle.js public/bundle.js.map && git commit --allow-empty -m 'Deploying' && git push heroku deploy:master",
    "deploy": "git pull heroku master && webpack -p && git add -f public/bundle.js public/bundle.js.map && git commit --allow-empty -m 'Deploying' && git push heroku deploy:master",
    "start": "node server",
    "start-dev": "npm run build-client-watch & npm run start-server",
    "start-server": "NODE_ENV='development' nodemon server -e html,js,scss --ignore public",
    "test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/slydv-test' mocha ./test/**/*.spec.js --compilers js:babel-register",
    "seed": "node seed"
  },
  "eslintConfig": {
    "globals": {
      "window": "true"
    }
  },
